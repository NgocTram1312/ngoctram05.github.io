        "# for loop to cube even numbers and square odd numbers\n",
        "cubes_and_squares = []\n",
        "for num in nums:\n",
        "    if num % 2 == 0:\n",
        "        cubes_and_squares.append(num**3)\n",
        "    else:\n",
        "        cubes_and_squares.append(num**2)"
      ],
      "metadata": {
        "id": "nRvONoSnL_uG"
      },
      "execution_count": null,
      "outputs": []
    },
